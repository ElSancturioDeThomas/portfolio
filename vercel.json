{
  "version": 2,
  "buildCommand": "bash portfolio/build.sh",
  "builds": [
    {
      "src": "portfolio/api/vercel.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "portfolio/api/vercel.py"
    }
  ]
}
